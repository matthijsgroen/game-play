{
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9.]+$": {
      "type": "object",
      "properties": {
        "attributes": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "height": { "type": "integer", "minimum": 0 },
            "width": { "type": "integer", "minimum": 0 },
            "scale": { "type": "number", "minimum": 0 },
            "rotation": { "type": "integer" },
            "ro": {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": [
                {
                  "title": "x offset",
                  "type": "integer"
                },
                {
                  "title": "y offset",
                  "type": "integer"
                }
              ]
            }
          },
          "required": ["width", "height"]
        },
        "spriteAttributes": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "accentColor": { "type": "string", "pattern": "^#[0-9a-f]{6}$" }
          }
        },
        "sprites": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "array",
            "minItems": 2,
            "maxItems": 2,
            "items": [
              { "title": "name of sprite", "type": "string" },
              {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "crop": {
                    "type": "array",
                    "minItems": 4,
                    "maxItems": 4,
                    "items": { "type": "integer" }
                  },
                  "key": { "type": "string" },
                  "attachTo": { "type": "string" },
                  "x": { "type": "integer" },
                  "y": { "type": "integer" },
                  "z": { "type": "integer" },
                  "h": { "type": "integer" },
                  "w": { "type": "integer" },
                  "alpha": { "type": "number", "minimum": 0, "maximum": 1 },
                  "scale": { "type": "number", "minimum": 0 },
                  "scaleX": { "type": "number", "minimum": 0 },
                  "scaleY": { "type": "number", "minimum": 0 },
                  "hideAbove": { "type": "number" },
                  "hideBelow": { "type": "number" },
                  "flipX": { "type": "boolean" },
                  "overrideColor": {
                    "type": "string",
                    "pattern": "^#[0-9a-f]{6}$"
                  },
                  "accentColor": {
                    "type": "string",
                    "pattern": "^#[0-9a-f]{6}$"
                  },
                  "horizon": {
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 2,
                    "items": { "type": "number" }
                  },
                  "ro": {
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 2,
                    "items": { "type": "integer" }
                  },
                  "rotation": { "type": "number" },
                  "hitbox": {
                    "type": "array",
                    "minItems": 6,
                    "items": { "type": "integer" }
                  }
                }
              }
            ]
          }
        },
        "gradients": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "key": { "type": "string" },
              "w": { "type": "integer" },
              "h": { "type": "integer" },
              "z": { "type": "integer" },
              "topColor": {
                "type": "array",
                "items": [
                  {
                    "type": "string",
                    "pattern": "^#[0-9a-f]{6}$"
                  },
                  {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1
                  }
                ],
                "additionalItems": false
              },
              "bottomColor": {
                "type": "array",
                "items": [
                  {
                    "type": "string",
                    "pattern": "^#[0-9a-f]{6}$"
                  },
                  {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1
                  }
                ],
                "additionalItems": false
              }
            },
            "required": ["w", "h", "topColor", "bottomColor"]
          }
        },
        "hitbox": {
          "type": "array",
          "minItems": 6,
          "items": { "type": "integer" }
        },
        "attachHooks": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "array",
            "items": [
              { "type": "string" },
              {
                "type": "object",
                "properties": {
                  "x": { "type": "integer" },
                  "y": { "type": "integer" },
                  "z": { "type": "integer" },
                  "attachTo": { "type": "string" },
                  "attachAlign": {
                    "type": "array",
                    "items": [
                      { "type": "string", "enum": ["top", "center", "bottom"] },
                      { "type": "string", "enum": ["left", "center", "right"] }
                    ],
                    "additionalItems": false
                  }
                },
                "additionalProperties": false
              }
            ],
            "additionalItems": false
          }
        },
        "frames": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9]+$": {
              "type": "object",
              "properties": {
                "flipX": {
                  "type": "boolean"
                },
                "attributes": {
                  "type": "object",
                  "properties": {
                    "scale": { "type": "number" },
                    "rotation": { "type": "integer" },
                    "w": { "type": "integer" },
                    "z": { "type": "integer" }
                  },
                  "additionalProperties": false
                }
              },
              "patternProperties": {
                "^(?!flipX)(?!attributes)[a-zA-Z0-9.]+$": {
                  "type": "object",
                  "properties": {
                    "h": { "type": "integer" },
                    "x": { "type": "integer" },
                    "y": { "type": "integer" },
                    "z": { "type": "integer" },
                    "rotation": { "type": "number" },
                    "overrideColor": {
                      "type": ["string", "null"],
                      "pattern": "^#[0-9a-f]{6}$"
                    },
                    "horizon": {
                      "type": "array",
                      "minItems": 2,
                      "maxItems": 2,
                      "items": { "type": "number" }
                    },
                    "hidden": { "type": "boolean" },
                    "scale": { "type": "number" },
                    "scaleX": { "type": "number" },
                    "scaleY": { "type": "number" },
                    "alpha": { "type": "number" },
                    "maxAlpha": { "type": "number" },
                    "sprite": { "type": "string" },
                    "topColor": {
                      "type": "array",
                      "items": [
                        {
                          "type": "string",
                          "pattern": "^#[0-9a-f]{6}$"
                        },
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 1
                        }
                      ],
                      "additionalItems": false
                    },
                    "bottomColor": {
                      "type": "array",
                      "items": [
                        {
                          "type": "string",
                          "pattern": "^#[0-9a-f]{6}$"
                        },
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 1
                        }
                      ],
                      "additionalItems": false
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "animations": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9.]+$": {
              "type": "object",
              "properties": {
                "repeat": { "type": "boolean" },
                "duration": { "type": "integer", "minimum": 0 },
                "easing": {
                  "type": "string",
                  "enum": ["linear", "easeOutQuad", "easeInQuad"]
                },
                "after": {
                  "type": "object",
                  "properties": {
                    "animation": { "type": "string" }
                  },
                  "additionalProperties": false
                },
                "startEase": {
                  "type": "object",
                  "properties": {
                    "easing": {
                      "type": "string",
                      "enum": ["linear", "easeOutQuad", "easeInQuad"]
                    },
                    "duration": { "type": "integer", "minimum": 0 }
                  },
                  "required": ["duration"],
                  "additionalProperties": false
                },
                "timer": { "type": "string", "enum": ["global"] },
                "timeline": {
                  "type": "array",
                  "minItems": 1,
                  "items": {
                    "type": "object",
                    "properties": {
                      "start": { "type": "number", "minimum": 0, "maximum": 1 },
                      "end": { "type": "number", "minimum": 0, "maximum": 1 },
                      "startFrame": { "type": "string" },
                      "endFrame": { "type": "string" },
                      "spriteAnimation": {
                        "type": "object",
                        "properties": {
                          "key": { "type": "string" },
                          "sprites": {
                            "type": "array",
                            "minItems": 1,
                            "items": { "type": "string" }
                          }
                        },
                        "required": ["key", "sprites"],
                        "additionalProperties": false
                      }
                    },
                    "required": ["start", "end"],
                    "additionalProperties": false
                  }
                }
              },
              "required": ["timeline", "duration"],
              "additionalProperties": false
            }
          },

          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
