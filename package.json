{
  "name": "SpeedLazer",
  "version": "2.0.0-pre",
  "description":
    "SpeedLazer is a HTML5 Side scrolling shoot-em-up made with Crafty.js",
  "license": "MIT",
  "homepage": "http://speedlazer.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/speedlazer/speedlazer"
  },
  "keywords": [
    "side-scroller",
    "HTML5",
    "Crafty.js",
    "SHMUP",
    "Gamepad",
    "WebGL"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "coffee-loader": "^0.9.0",
    "coffeescript": "1.12.7",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.12.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.0.1",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "start": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "lint": "eslint src/",
    "test":
      "mocha --recursive --require @babel/polyfill --require @babel/register",
    "prettier": "prettier '{,!(node_modules|dist|.tmp)/**/}*.js' --write"
  },
  "babel": {
    "presets": ["@babel/env"],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "Crafty-Distro": "craftyjs/Crafty-Distro#nightlies",
    "bezier-js": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.4",
    "noisejs": "^2.1.0",
    "preact": "^8.4.2",
    "when": "^3.7.8"
  },
  "prettier": {
    "tabWidth": 2
  }
}
